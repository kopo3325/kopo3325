#include <stdio.h>

void main()
{
    int n;
    
    printf("숫자를 입력하세요.");
    scanf("%d", &n);
    
    for (int i = 0; i < n; i++) { //줄 반복문
        for (int j = n; j > i; j--) { // 공백 반복문
            printf(" ");
        }    
        for (int k = 0; k <= i; k++) { //숫자 나열 반복문
            printf(" %d", k + 1);
        }
    printf("\n");
    }
} 
