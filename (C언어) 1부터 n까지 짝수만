#include <stdio.h>

void main()
{
    int n;
    int i = 0;
    printf("숫자를 입력하세요 : ");
    scanf("%d", &n);
    
    if (n <= 0) {
        printf("0보다 큰 자연수를 입력하세요.");
    } else {
        while (i <= (n - 1)) {
            i++;
            if (i % 2 == 1) {
                continue;
            } else {
                printf("%d ", i);
            }
        }    
    }
}
