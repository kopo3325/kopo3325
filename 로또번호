#include <stdio.h>
#include <stdlib.h> //rand, srand 함수수
#include <time.h> //시간 함수
#include <unistd.h> //sleep 함수

int main()
{
    int i;
    int j;
    int temp;
    int num[6];
    
    srand(time(NULL) + i);//현재시간을 기준으로 i값을 더하여 매번 다른 값 생성
    
    for (i = 0; i < 6; i++)
    {
        num[i] = (int)(rand() % 45) + 1; //
    
        for (j = 0; j < i; j++) 
        {
            if(num[i] == num[j])
            {
                i--;
                break;
            }
        
        }
    }
    
    for (i = 0; i < 6; i++) //num배열의 0부터 5까지 반복되는 루프(6번 반복)
    {
        for (j = i + 1; j < 6; j++)
        {
            
            if (num[i] > num[j])
            {
                temp = num[i];
                num[i] = num[j];
                num[j] = temp;
            }
      
        }
    }
    for ( i = 0; i < 6; i++)
    {
        printf("%d\n", num[i]);
        sleep(1);
    }
    return 0;
}
